{
    "name": "Domain Classifier",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Extension to classify websites by their domain extension",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_icon": "integrity.png",
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["response.json"],
            "matches": ["<all_urls>"]
        }
    ]
}
